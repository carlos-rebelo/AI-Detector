{
  "manifest_version": 3,
  "name": "AI Content Detector",
  "version": "1.0",
  "description": "Detects whether text on a webpage is AI-generated.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
      },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
